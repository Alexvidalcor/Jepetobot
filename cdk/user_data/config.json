{
	"agent": {
		"metrics_collection_interval": 60,
		"run_as_user": "root"
	},
	"logs": {
		"logs_collected": {
			"files": {
				"collect_list": [
					{
						"file_path": "/var/log/application/app.log",
						"log_group_name": "REPLACEAPPNAME-log_group",
						"log_stream_name": "{instance_id}-REPLACEAPPNAME-app",
						"retention_in_days": 7
					},
					{
						"file_path": "/var/log/application/errors.log",
						"log_group_name": "REPLACEAPPNAME-log_group",
						"log_stream_name": "{instance_id}-REPLACEAPPNAME-errors",
						"retention_in_days": 7
					},
					{
						"file_path": "/var/log/application/user.log",
						"log_group_name": "REPLACEAPPNAME-log_group",
						"log_stream_name": "{instance_id}-REPLACEAPPNAME-user",
						"retention_in_days": 7
					}
				]
			}
		}
	}
}
